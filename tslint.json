{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [true, 1000],
    "import-name": false,
    "function-name": false,
    "object-shorthand-properties-first": false,
    "import/extensions": [
      "error",
      "always",
      {
        "js": "never",
        "vue": "never",
        "ts": "never"
      }
    ]
  }
}
